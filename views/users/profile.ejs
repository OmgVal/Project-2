<h2> Welcome to your profile!! </h2>

<!-- Add for each loop () -->

<div>
    <% gifs.forEach(gif => { %>
    <a href="/gifs/details/<%= gif.id %>">
            <% if (gif.images.original.url != " ") { %>
            <img  src="<%= gif.images.original.url %>" alt="<%= gif.title %>"/> 
            <% } %>
    </a>
    <% }) %>
</div>