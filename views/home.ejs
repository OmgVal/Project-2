<div class="container">
    <h2 class="text-center m-5"> Trending ðŸ¥³</h2>
    
    
    <div class="d-flex justify-content-md-center p-2">
        <div class="row">
            <% gifs.forEach(gif => { %>
                <div class="col d-inline-flex">
                    <a href="/gifs/details/<%= gif.id %>">
                <% if (gif.images.original.url != " ") { %>
                    <img id="gifImg" class="rounded d-block m-0" src="<%= gif.images.original.url %>" alt="<%= gif.title %>"/> 
                    <% } %>
                </a>
            </div>
            <% }) %>
        </div>   
    </div>
</div>
  

        
